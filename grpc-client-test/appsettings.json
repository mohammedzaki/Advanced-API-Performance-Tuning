{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Debug"
    }
  },
  "AllowedHosts": "*",
  "OTEL_SERVICE_NAME": "grpc-client-test",
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://otel-collector:4318/v1/traces",
  "GrpcClient": {
    "ServiceUrl": "http://dotnet-app:8083",
    "Timeout": 30000,
    "EnableReflection": true,
    "EnableDetailedErrors": true
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080",
        "Protocols": "Http1"
      }
    }
  }
}